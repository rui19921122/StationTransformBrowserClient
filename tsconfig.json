{
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": "./",
    "moduleResolution": "node",
    "target": "es6",
    "noImplicitAny": false,
    "sourceMap": true,
    "jsx": "preserve",
    "removeComments": true,
    "allowSyntheticDefaultImports": true
  },
  "compileOnSave": true,
  "include":[
    "src"
  ],
  "exclude": [
    "dist",
    "typings",
    "node_modules"
  ],
  "files": [
    "typings/index.d.ts"
  ]
}
